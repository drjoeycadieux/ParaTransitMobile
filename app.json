{
  "expo": {
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "name": "para-transit",
    "slug": "para-transit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rideQ.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/para-transit.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/para-transit.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.paratransit"
    },
    "web": {
      "favicon": "./assets/para-transit.png"
    },
    "extra": {
      "eas": {
        "projectId": "b1deace4-1b91-49f6-9ffb-02c7cc8a965a"
      }
    }
  }
}
